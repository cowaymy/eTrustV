@charset"utf-8";

/* 레이아웃 */
#wrap{width:100%; min-width:1200px; height:100%;}
#container{position:absolute; top:0; left:0; width:100%; min-width:1200px; height:100%; padding:32px 0 0 215px; box-sizing:border-box;}
#container #content{padding:10px 10px 10px 23px;}
#container #content:after{content:""; display:block; clear:both;}

/* 헤더 */
#header{position:fixed; top:0; left:0; z-index:100; width:100%; min-width:1200px; height:32px; background:#a1c4d7;}
#header:after{content:""; display:block; clear:both;}
#header .left_opt{float:left; margin-left:20px;}
#header .left_opt:after{content:""; display:block; clear:both;}
#header .left_opt li{float:left; margin-right:14px; line-height:32px; font-size:11px; color:#415c6f;}
#header .left_opt li span{line-height:32px; font-size:11px; color:#415c6f; font-weight:bold;}
#header .right_opt{float:right; margin-right:10px;}
#header .right_opt:after{content:""; display:block; clear:both;}
#header .right_opt li{float:left;line-height:32px; font-size:11px; color:#42616e;}
#header .right_opt li span{line-height:32px; font-size:11px; color:#42616e; font-weight:bold;}
#header .right_opt li a{display:block; margin:4px 0 0 5px;}
#header .right_opt li a.logout{position:relative; padding-left:11px; margin:0 15px 0 10px; line-height:32px; font-size:11px; color:#42616e;}
#header .right_opt li a.logout:before{position:absolute; top:11px; left:0; content:""; display:block; width:1px; height:9px; background:#7fa0b2;}

/* lnb */
.lnb_wrap{position:fixed; top:0; left:0; z-index:90; width:215px; height:100%; padding:32px 0 0 0; box-sizing:border-box; background:#25527c;}
.lnb_wrap .lnb_header{position:relative; z-index:10; padding:16px 19px 15px 19px;}
.lnb_wrap .lnb_header h1{margin-bottom:9px; text-align:center;}
.lnb_wrap .lnb_header .search{position:relative;}
.lnb_wrap .lnb_header input[type=text]{width:100%; height:30px; line-height:30px; border:0 none; box-sizing:border-box; border-radius:3px; text-indent:10px;}
.lnb_wrap .lnb_header input[type=image]{position:absolute; top:6px; right:5px;}
.lnb_wrap .lnb_con{position:absolute; top:0; left:0; z-index:9; width:100%; height:100%; padding:160px 0 0 0; box-sizing:border-box;}
.lnb_wrap .lnb_con > p{position:absolute; top:126px; right:0;  width:50%;}
.lnb_wrap .lnb_con > p:first-child{left:0;}
.lnb_wrap .lnb_con > p a{display:block; height:34px; line-height:34px; font-size:14px; color:#6d89a3; background:#0c3a65; text-align:center;}
.lnb_wrap .lnb_con > p.on a{background:#06a7e2; color:#fff;}
.lnb_wrap .lnb_con > p a span{position:relative; top:2px; display:inline-block; width:14px; height:15px; line-height:34px; margin:0 5px 0 0; background:url(../image/icon_fav.gif) no-repeat; background-position:0 0;}
.lnb_wrap .lnb_con > p.on a span{background-position:100% 0;}
.lnb_wrap .lnb_con .inb_menu{display:none; width:100%; height:100%; border:4px solid #06a7e2; background:#fff; box-sizing:border-box; overflow:auto;}
.lnb_wrap .lnb_con > p.on+.inb_menu{display:block;} 
.lnb_wrap .lnb_con .inb_menu > li{border-bottom:1px solid #e2e2e2;}
.lnb_wrap .lnb_con .inb_menu > li > a{display:block; line-height:30px; padding:0 15px; color:#666; background:#fff url(../image/lnb_arrow_off.gif) no-repeat right center;}
.lnb_wrap .lnb_con .inb_menu > li.on > a{color:#fff; background:#06a7e2 url(../image/lnb_arrow_on.gif) no-repeat center right;}
.lnb_wrap .lnb_con .inb_menu > li > ul > li > a{display:block; line-height:26px; padding:0 15px 0 31px; font-size:12px; color:#25527c; border-top:1px solid #e2e2e2; background:#f0f0f0 url(../image/lnb_arrow_sub_off.gif) no-repeat 20px center;}
.lnb_wrap .lnb_con .inb_menu > li > ul > li.on  > a{background:#e2e2e2; background:#f0f0f0 url(../image/lnb_arrow_sub_on.gif) no-repeat 20px center; font-weight:bold;}
.lnb_wrap .lnb_con .inb_menu > li > ul > li:first-child > a{border-top:0 none;}
.lnb_wrap .lnb_con .inb_menu > li > ul > li > ul{padding:4px 0;  background:#f0f0f0; border-top:1px solid #e2e2e2;}
.lnb_wrap .lnb_con .inb_menu > li > ul > li > ul > li > a{display:block; line-height:20px; padding:0 15px 0 39px; font-size:11px; color:#25527c;}
.lnb_wrap .lnb_con .inb_menu > li > ul > li > ul > li.on > a{font-weight:bold;}

/* content 공통 */
.path{float:right; margin-bottom:15px;}
.path:after{content:""; display:block; clear:both;}
.path li{position:relative; float:left; padding-left:19px; color:#a9b4b7;}
.path li:before{content:""; display:block; position:absolute; top:6px; left:7px; width:6px; height:5px; background:url(../image/path_arrow.gif) no-repeat;}
.path li:first-child:before{display:none;}
.path li:last-child{font-weight:bold;}
.title_line{clear:both; margin-bottom:10px;}
.title_line:after{content:""; display:block; clear:both;}
.title_line .fav{position:relative; top:2px; float:left; margin-right:10px;}
.title_line h2{float:left; font-size:24px; color:#25527c; font-weight:normal;}
.title_line .right_opt{float:right;}
.title_line .right_opt:after{content:""; display:block; clear:both;}
.title_line .right_opt li{float:left; margin-left:5px;}
.h70{height:70px;}

/* 각종 버튼 */
.btn_blue a{display:inline-block; min-width:57px; height:24px; line-height:24px; padding:2px 10px; background:#25527c; color:#fff; font-size:11px; border-radius:3px; text-align:center;}
.btn_blue.multy a{line-height:12px;}
.btn_blue2 a{display:inline-block; min-width:57px; height:28px; line-height:28px; padding:0 10px; background:#25527c; color:#fff; font-size:11px; border-radius:3px; text-align:center; font-weight:bold;}
.btn_blue2.big a{min-width:120px; height:36px; line-height:36px; font-size:14px;}
.btn_sky a{display:inline-block; min-width:57px; height:30px; line-height:30px; padding:0 10px; background:#a1c4d7; color:#fff; font-size:12px; font-weight:bold; border-radius:3px; text-align:center;}
.btn_gray a{display:inline-block; height:30px; line-height:30px; padding:0 20px; background:#999; color:#fff; font-size:12px; font-weight:bold; border-radius:3px; text-align:center;}
.btn_gray a .search{display:inline-block; width:10px; height:9px; margin-right:5px; background:url(../image/btn_search.gif) no-repeat;}
.btn_gray a .clear{display:inline-block; width:11px; height:11px; margin-right:5px; background:url(../image/btn_clear.gif) no-repeat;}
.btn_grid a{display:inline-block; min-width:60px;height:22px; line-height:22px; padding:0 10px; background:#e0e8ec; border:1px solid #90a9b7; color:#25527c; font-size:10px; font-weight:bold; border-radius:3px; border-radius:3px; text-align:center;}
.right_btns{text-align:right;}
.right_btns li{display:inline-block;}
.center_btns{text-align:center;}
.center_btns li{display:inline-block;}
table+.center_btns{padding-top:15px;}
.grid_wrap{height:500px; margin:5px 0 20px 0;}
.grid_wrap.h220{height:220px;}

/* 검색 영역 */
.search_table{margin-bottom:20px;}
.date_set{position:relative; width:100%;}
.date_set > p{float:left; width:50%; padding-left:11px; box-sizing:border-box;}
.date_set > p:first-child{padding-left:0; padding-right:11px;}
.date_set > p input[type=text]{width:100%!important;}
.date_set > span{position:absolute; top:0; left:50%; width:22px; margin-left:-11px; line-height:30px; text-align:center;}

/* 테이블 */
table.type1{margin-bottom:5px; border-top:1px solid #d3d9dd;}
table.type1 tbody th{padding-left:15px; color:#333; font-weight:bold; background:#e9f0f4; text-align:left; border-bottom:1px solid #d3d9dd; vertical-align:top; line-height:34px;}
table.type1 tbody td{padding:2px 6px; border-bottom:1px solid #d3d9dd;}
table.type1 tbody td > *{float:left; margin-right:4px;}
table.type1 tbody td > span{line-height:30px;}
table.type1 tbody td > span.multy{line-height:15px;}
table.type1 tbody td input[type=text]{width:237px; height:30px; line-height:28px; border:1px solid #d2d2d2; box-sizing:border-box; border-radius:3px; text-indent:6px;}
table.type1 tbody td select{width:237px; height:30px; line-height:26px; padding:0 0 0 4px; margin:0; border:1px solid #d2d2d2; border-radius:3px; box-sizing:border-box; font-size:13px; color:#444; font-family:Verdana,"돋움",Dotum,sans-serif;}
table.type1 tbody td .w100p{width:100%!important;}
table.type2{border-top:1px solid #d3d9dd;}
table.type2 thead th{height:35px; color:#333; font-weight:bold; line-height:15px; background:#e9f0f4; border-bottom:1px solid #d3d9dd; border-left:1px solid #d3d9dd; vertical-align:middle;}
table.type2 thead tr th:first-child{border-left:0 none;}
table.type2 tbody td{height:31px; padding:2px 6px; border-bottom:1px solid #d3d9dd; border-left:1px solid #d3d9dd; text-align:center; vertical-align:middle;}
table.type2 tbody tr td:first-child{border-left:0 none;}
table.type2 tbody td input[type=text]{width:100%; height:30px; line-height:28px; border:1px solid #d2d2d2; box-sizing:border-box; border-radius:3px; text-indent:6px;}
table.type2 tbody td.al_left{text-align:left;}
table.type2.gray{border-top:2px solid #111;}
table.type2.gray thead th{color:#333; background:#eee;}
table.type2.gray tbody td{padding:2px 10px;}
.page_size{padding:20px 0 10px 0;}
.page_size:after{content:""; display:block; clear:both;}
.page_size p{float:left; font-size:12px; line-height:18px; margin-right:10px;}
.page_size select{float:left;  height:20px; border:1px solid #d2d2d2; border-radius:0; box-sizing:border-box; font-size:11px; color:#444; font-family:Verdana,"돋움",Dotum,sans-serif;}
.table_bottom_info{padding-top:10px; font-size:12px;}
.table_bottom_info span{font-size:12px; font-weight:bold;}

/* 제이쿼리 달력 */
.ui-datepicker-header{position:relative; width:178px; padding:10px; border:1px solid #ccc; text-align:center; background:#f3f3f3;}
.ui-datepicker-prev,
.ui-datepicker-next{position:absolute; top:14px; display:block; width:15px; height:15px; text-indent:-1000em; overflow:hidden; background:url(../image/btn_cal_arr.gif) no-repeat; cursor:pointer;}
.ui-datepicker-prev{left:10px;}
.ui-datepicker-next{right:10px; background-position:100% 0;}
.ui-datepicker-title{font-weight:bold; line-height:15px; color:#5a7f96;}
.ui-datepicker-month{margin-right:5px;}
.ui-datepicker-calendar{width:200px; padding:10px; border:1px solid #ccc; border-top:0 none; background:#fff;}
.ui-datepicker-calendar th,
.ui-datepicker-calendar td{width:25px; height:20px; line-height:20px; text-align:center;}
.ui-datepicker-calendar th{color:#000;}
.ui-datepicker-calendar td{background:#f3f3f3; vertical-align:middle;}
.ui-datepicker-calendar td:hover{background:#d4efff;}
.ui-datepicker-calendar td a:hover{text-decoration:none; color:#5a7f96;}
.ui-datepicker-calendar thead th:first-child{color:#cf0303!important;}
.ui-datepicker-calendar .ui-datepicker-week-end{color:#0865ff;}
.ui-datepicker-calendar .ui-datepicker-week-end a{color:#0865ff;}
.ui-datepicker-calendar tbody tr td:first-child a{color:#cf0303!important;}

/* 팝업 */
#popup_wrap{max-width:1000px; width:100%; margin:auto;}
#popup_wrap .pop_header{padding:20px 20px 10px 20px; background:#e5e5e5; border-bottom:2px solid #25527c;}
#popup_wrap .pop_header:after{content:""; display:block; clear:both;}
#popup_wrap .pop_header h1{position:relative; float:left; height:28px; padding-left:14px; font-size:24px; color:#111; font-weight:normal;}
#popup_wrap .pop_header h1:before{content:""; display:block; position:absolute; top:3px; left:0; width:3px; height:19px; background:#25527c;}
#popup_wrap .pop_header .right_opt{float:right;}
#popup_wrap .pop_header .right_opt:after{content:""; display:block; clear:both;}
#popup_wrap .pop_header .right_opt li{float:left; margin-left:5px;}
#popup_wrap .pop_body{padding:20px;}
#popup_wrap .pop_body h2{padding:10px 0; font-size:14px; color:#25527c;}

/* 탭 */
.tap_type1{margin-bottom:10px;}
.tap_type1:after{content:""; display:block; clear:both;}
.tap_type1 li{float:left; margin-right:-1px;}
.tap_type1 li a{display:block; height:29px; line-height:28px; padding:0 19px; border:1px solid #d3d9dd; font-size:12px; color:#888; font-weight:bold; border-radius:0 8px 0 0;}
.tap_type1 li a.on{background:#a1c4d7; border:1px solid #a1c4d7; color:#25527c;}

/* 페이징 */
.pag{position:relative; padding:10px 0 0 0; text-align:center;}
.pag .first a,
.pag .last a,
.pag .prev a,
.pag .next a{display:block; width:21px; height:21px; text-indent:-1000em; background:url(../image/btn_pag.gif) no-repeat; overflow:hidden;}
.pag .prev a{background-position:-22px 0;}
.pag .next a{background-position:-44px 0;}
.pag .last a{background-position:-66px 0;}
.pag .first,
.pag .last,
.pag .prev,
.pag .next{display:inline-block; vertical-align:top; margin:0 1px;}
.pag > ol{display:inline-block; vertical-align:top;}
.pag > ol:after{content:""; display:block; clear:both;}
.pag > ol > li{float:left;}
.pag > ol > li a{display:inline-block; width:20px; line-height:20px; font-size:12px;}
.pag > ol > li a.on{font-weight:bold; color:#25527c;}